            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"


                android:orientation="vertical">

                <!--<com.bm.main.materialedittext.MaterialEditText-->
                <!--android:id="@+id/materialEditTextIdUpline"-->
