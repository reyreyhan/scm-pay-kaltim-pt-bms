<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:orientation="vertical"
    tools:context="com.bm.main.single.ftl.activities.TravelTanggalPesananActivity">
    <include
        android:id="@+id/toolbar"
        layout="@layout/core_toolbar"
        />
    <com.prolificinteractive.materialcalendarview.MaterialCalendarView
        android:id="@+id/calendarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:mcv_leftArrow="@drawable/ms_ic_chevron_left"
        app:mcv_rightArrow="@drawable/ms_ic_chevron_right"
        app:mcv_selectionColor="@color/md_blue_700"
        app:mcv_showOtherDates="out_of_range|other_months"
        />
</LinearLayout>